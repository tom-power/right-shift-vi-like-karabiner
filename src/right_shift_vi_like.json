{
  "title": "right shift vi like",
  "rules": [
    {
      "description": "Right (right_shift+l to right)",
      "manipulators": [
        {
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": ["right_shift"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Down (right_shift+j to down)",
      "manipulators": [
        {
          "from": {
            "key_code": "j",
            "modifiers": {
              "mandatory": [ "right_shift" ],
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "down_arrow"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Left (right_shift+h to left)",
      "manipulators": [
        {
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [ "right_shift" ],
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Up (right_shift+k to up)",
      "manipulators": [
        {
          "from": {
            "key_code": "k",
            "modifiers": {
              "mandatory": [ "right_shift" ],
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "up_arrow"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "End (right_shift+e to right_command+right)",
      "manipulators": [
        {
          "from": {
            "key_code": "e",
            "modifiers": {
              "mandatory": ["right_shift"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "right_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Home (right_shift+a to right_command+left)",
      "manipulators": [
        {
          "from": {
            "key_code": "a",
            "modifiers": {
              "mandatory": ["right_shift"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "right_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Word forward (right_shift+w to option+right)",
      "manipulators": [
        {
          "from": {
            "key_code": "w",
            "modifiers": {
              "mandatory": [ "right_shift" ],
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "left_option"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Word back (right_shift+b to option+left)",
      "manipulators": [
        {
          "from": {
            "key_code": "b",
            "modifiers": {
              "mandatory": [ "right_shift" ],
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "left_option"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Page up (right_shift+p to page_up)",
      "manipulators": [
        {
          "from": {
            "key_code": "p",
            "modifiers": {
              "mandatory": [ "right_shift" ],
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "page_up"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Page up 2 (left_control+backspace to page_up)",
      "manipulators": [
        {
          "from": {
            "key_code": "backspace",
            "modifiers": {
              "mandatory": [ "left_control" ],
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "page_up"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Page down (right_shift+; to page_down)",
      "manipulators": [
        {
          "from": {
            "key_code": "semicolon",
            "modifiers": {
              "mandatory": [ "right_shift" ],
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "page_down"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Page down 2 (left_control+enter to page_down)",
      "manipulators": [
        {
          "from": {
            "key_code": "left_control",
            "modifiers": {
              "mandatory": [ "return_or_enter" ],
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "page_down"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Copy (right_shift+c to c+command)",
      "manipulators": [
        {
          "from": {
            "key_code": "c",
            "modifiers": {
              "mandatory": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "c",
              "modifiers": [
                "right_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Cut (right_shift+x to x+command)",
      "manipulators": [
        {
          "from": {
            "key_code": "x",
            "modifiers": {
              "mandatory": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "x",
              "modifiers": [
                "right_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Paste (right_shift+v to v+command)",
      "manipulators": [
        {
          "from": {
            "key_code": "v",
            "modifiers": {
              "mandatory": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "v",
              "modifiers": [
                "right_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Undo (right_shift+z to z+command)",
      "manipulators": [
        {
          "from": {
            "key_code": "z",
            "modifiers": {
              "mandatory": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "z",
              "modifiers": [
                "right_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Save (right_shift+s to s+command)",
      "manipulators": [
        {
          "from": {
            "key_code": "s",
            "modifiers": {
              "mandatory": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "s",
              "modifiers": [
                "right_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Info (right_shift+i to i+command)",
      "manipulators": [
        {
          "from": {
            "key_code": "i",
            "modifiers": {
              "mandatory": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "i",
              "modifiers": [
                "right_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "New tab (right_shift+t to t+command)",
      "manipulators": [
        {
          "from": {
            "key_code": "t",
            "modifiers": {
              "mandatory": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "t",
              "modifiers": [
                "right_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Close tab (right_shift+y to w+command)",
      "manipulators": [
        {
          "from": {
            "key_code": "y",
            "modifiers": {
              "mandatory": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "w",
              "modifiers": [
                "right_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Escape (left_command+space to escape)",
      "manipulators": [
        {
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [ "left_command" ],
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "escape"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Return (right_shift+m to return)",
      "manipulators": [
        {
          "from": {
            "key_code": "m",
            "modifiers": {
              "mandatory": [ "right_shift" ],
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "return_or_enter"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Delete (right_shift+f to delete)",
      "manipulators": [
        {
          "from": {
            "key_code": "f",
            "modifiers": {
              "mandatory": [ "right_shift" ],
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "delete_forward"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Backspace (right_shift+d to backspace)",
      "manipulators": [
        {
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "delete_or_backspace"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Tab (right_shift+space to tab)",
      "manipulators": [
        {
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": [ "right_shift" ],
              "optional": [ "any" ]
            }
          },
          "to": [
            {
              "key_code": "tab"
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Go back (right_shift+cmd+option+h to cmd+[)",
      "manipulators": [
        {
          "from": {
            "key_code": "h",
            "modifiers": {
              "mandatory": [
                "right_shift",
                "left_command",
                "left_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket",
              "modifiers": [
                "left_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Go forward (right_shift+cmd+option+l to cmd+])",
      "manipulators": [
        {
          "from": {
            "key_code": "l",
            "modifiers": {
              "mandatory": [
                "right_shift",
                "left_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "close_bracket",
              "modifiers": [
                "left_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Go to tab to left (right_shift+u to cmd+shift+[)",
      "manipulators": [
        {
          "from": {
            "key_code": "u",
            "modifiers": {
              "mandatory": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "open_bracket",
              "modifiers": [
                "left_command",
                "left_shift"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Go to tab to right (right_shift+o to cmd+shift+])",
      "manipulators": [
        {
          "from": {
            "key_code": "o",
            "modifiers": {
              "mandatory": [
                "right_shift"
              ]
            }
          },
          "to": [
            {
              "key_code": "close_bracket",
              "modifiers": [
                "left_command",
                "left_shift"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Spotlight (left_command+return to left_command+spacebar)",
      "manipulators": [
        {
          "from": {
            "key_code": "return_or_enter",
            "modifiers": {
              "mandatory": [
                "left_command"
              ]
            }
          },
          "to": [
            {
              "key_code": "spacebar",
              "modifiers": [
                "left_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}